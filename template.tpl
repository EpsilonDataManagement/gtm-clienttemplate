___SANDBOXED_JS_FOR_SERVER___

// Imports
const addMessageListener = require('addMessageListener');
const addEventCallback = require('addEventCallback');
const claimRequest = require('claimRequest');
const extractEventsFromMpv2 = require('extractEventsFromMpv2');
const getRemoteAddress = require('getRemoteAddress');
const getRequestHeader = require('getRequestHeader');
const setResponseHeader = require('setResponseHeader');
const setResponseStatus = require('setResponseStatus');
const isRequestMpv2 = require('isRequestMpv2');
const logToConsole = require('logToConsole');
const returnResponse = require('returnResponse');
const runContainer = require('runContainer');
const setCookie = require('setCookie');
const setPixelResponse = require('setPixelResponse');
const sendPixelFromBrowser = require('sendPixelFromBrowser');

const JSON = require('JSON');

// Parsing Values
const ua = getRequestHeader('user-agent');
const ip = getRemoteAddress();

if (isRequestMpv2()) {
  claimRequest();
  
  const events = extractEventsFromMpv2();
  
  // Message Listener for signalling tag response completion
  addMessageListener('dtm_complete', (messageType, message) => {
    logToConsole('Client Message Listener:' + JSON.stringify(message));
    
    if (message.status === 'success') {
      logToConsole('Setting response cookies - total:' + message.cookies.length);
      message.cookies.forEach(cookie =>  {
        logToConsole('Cookie Object:' + JSON.stringify(cookie));
        setCookie(cookie.name,      
                  cookie.value,                                
                  {'max-age': cookie.maxAge,                         
                   'domain': cookie.domain,                              
                   'path': '/',                          
                   'secure': true,                        
                   'sameSite': 'lax'                    
                  });
      });

      setResponseStatus(200);
    } else {
      setResponseStatus(500);
    }
  });  
  
  let eventsCompleted = 0;
  
  events.forEach((event, i) => {
    // Unless the event had IP and user-agent overrides, manually
    // override them with the IP and user-agent from the request
    // That way the GA collect call will appear to have originated
    // from the user's browser / device.
    if (ip) {
      event.ip_override = ip;
    }
    if (ua) {
      event.user_agent = ua;
    }
    
    // Pass the event to a virtual container
    runContainer(event, (bindToEvent) => {
      bindToEvent(addEventCallback)((containerId, eventData) => {
        eventData.tags.forEach((tag) => {
          logToConsole('Tag ID: ' + tag.id);
          logToConsole('Tag Status: ' + tag.status);
          logToConsole('Tag Execution Time: ' + tag.executionTime);
        });
      
        if (events.length === ++eventsCompleted) {
          
          // CORS
          logToConsole("Setting final response");
          const origin = getRequestHeader('Origin');
	  	  if (origin) {
            setResponseHeader('Access-Control-Allow-Origin', origin);
            setResponseHeader('Access-Control-Allow-Credentials', 'true');
          }
          
          // Need this to send response back to GTM client
          returnResponse();
        }
      });
    });
  });
}


